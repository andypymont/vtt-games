    "button-new-round": {
        "id": "button-new-round",
        "type": "button",
        "parent": "cards-holder",
        "y": -50,
        "x": 5,
        "width": 100,
        "height": 40,
        "text": "New Round",
        "clickRoutine": [
            {
                "func": "RECALL",
                "holder": "cards-holder"
            },
            {
                "func": "FLIP",
                "holder": "cards-holder",
                "face": 0
            },
            {
                "func": "SHUFFLE",
                "holder": "cards-holder"
            },
            {
                "func": "RECALL",
                "holder": "holder-elephant-tiles"
            },
            {
                "func": "RECALL",
                "holder": "holder-lion-tiles"
            },
            {
                "func": "RECALL",
                "holder": "holder-zebra-tiles"
            },
            {
                "func": "RECALL",
                "holder": "holder-leopard-tiles"
            },
            {
                "func": "RECALL",
                "holder": "holder-rhino-tiles"
            },
            "var total1 = ${PROPERTY text OF player1-score} + ${PROPERTY text OF player1-total}",
            {
                "func": "LABEL",
                "label": "player1-total",
                "mode": "set",
                "value": "${total1}"
            },
            {
                "func": "LABEL",
                "label": "player1-score",
                "mode": "set",
                "value": 0
            },
            "var total2 = ${PROPERTY text OF player2-score} + ${PROPERTY text OF player2-total}",
            {
                "func": "LABEL",
                "label": "player2-total",
                "mode": "set",
                "value": "${total2}"
            },
            {
                "func": "LABEL",
                "label": "player2-score",
                "mode": "set",
                "value": 0
            },
            "var total3 = ${PROPERTY text OF player3-score} + ${PROPERTY text OF player3-total}",
            {
                "func": "LABEL",
                "label": "player3-total",
                "mode": "set",
                "value": "${total3}"
            },
            {
                "func": "LABEL",
                "label": "player3-score",
                "mode": "set",
                "value": 0
            },
            "var total4 = ${PROPERTY text OF player4-score} + ${PROPERTY text OF player4-total}",
            {
                "func": "LABEL",
                "label": "player4-total",
                "mode": "set",
                "value": "${total4}"
            },
            {
                "func": "LABEL",
                "label": "player4-score",
                "mode": "set",
                "value": 0
            },
            "var total5 = ${PROPERTY text OF player5-score} + ${PROPERTY text OF player5-total}",
