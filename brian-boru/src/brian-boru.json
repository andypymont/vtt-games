  "hand-right": {
    "id": "hand-right",
    "type": "holder",
    "childrenPerOwner": true,
    "layer": -3,
    "x": 800,
    "y": 850,
    "width": 800,
    "height": 150,
    "dropOffsetX": 0,
    "dropOffsetY": 0,
    "stackOffsetX": 100,
    "stackOffsetY": 0,
    "onEnter": {
      "activeFace": 1
    },
    "countRoutine": [
      {
        "func": "FOREACH",
        "in": [
          "p1-seat",
          "p2-seat",
          "p3-seat",
          "p4-seat",
          "p5-seat"
        ],
        "loopRoutine": [
          {
            "func": "GET",
            "collection": [
              "${value}"
            ],
            "property": "player",
            "variable": "pID"
          },
          {
            "func": "SELECT",
            "property": "parent",
            "value": "${PROPERTY id}"
          },
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "property": "owner",
            "value": "${pID}",
            "collection": "cardsInHand"
          },
          {
            "func": "COUNT",
            "collection": "cardsInHand"
          },
          "var labelNum = ${key} + 1",
          "var labelPlayer = 'p' + ${labelNum}",
          "var labelName = ${labelPlayer} + '-hand-counter-right-value'",
          {
            "func": "LABEL",
            "collection": [
              "${labelName}"
            ],
            "mode": "SET",
            "value": "${COUNT}"
          }
        ]
      }
    ],
    "enterRoutine": [
      {
        "func": "CALL",
        "routine": "countRoutine"
      }
    ],
    "leaveRoutine": [
      {
        "func": "CALL",
        "routine": "countRoutine"
      }
    ]
  },
